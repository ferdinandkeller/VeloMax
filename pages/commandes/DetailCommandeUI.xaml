<Page
    x:Class="VéloMax.pages.DetailCommandeUI"
    xmlns:local="using:VéloMax.pages"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:controls="using:Microsoft.Toolkit.Uwp.UI.Controls"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <StackPanel Margin="25">
        <TextBlock Name="Content" MaxLines="20" Margin="0 0 0 25" />
        <Button Name="BackButton" Click="Retour" Content="Revenir en arrière" Background="#A1573A" Foreground="White" />
    </StackPanel>
</Page>
    
